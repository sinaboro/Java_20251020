package test;

public class Sample2 {

	public static void main(String[] args) {
		
		Person p =  new Person();
		
		p.name = "김대철";
		p.age = 20;
		
		System.out.println(p.name);
		System.out.println(p.age);
	}

}


class Person{
	
	String name;
	int age;	
}

